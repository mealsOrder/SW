// 키 바인딩을 이 파일에 넣어서 기본값을 덮어씁니다.
[
    //컴파일
    { "key": "ctrl+alt+c", "command": "workbench.action.tasks.build" },
    
    //실행
    { "key": "ctrl+alt+r", "command": "workbench.action.tasks.test" },
    
    // 새파일 만들기
    { "key": "ctrl+n",
      "command": "explorer.newFile",
      "when": "!editorFocus" 
    },
    
    // 새폴더 만들기
    { "key": "ctrl+shift+n",
      "command": "explorer.newFolder",
      "when": "!editorFocus" 
    }
]